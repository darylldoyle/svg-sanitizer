{
    "name": "t3g/svg-sanitize-elts7",
    "description": "An SVG sanitizer for PHP",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Daryll Doyle",
            "email": "daryll@enshrined.co.uk"
        },
        {
            "name": "TYPO3 GmbH",
            "email": "info@typo3.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^6.5"
    },
    "replace": {
        "enshrined/svg-sanitize": "*"
    },
    "autoload": {
        "psr-4": {
            "enshrined\\svgSanitize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "enshrined\\svgSanitize\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --no-coverage",
        "test:coverage": "phpunit"
    },
    "conflicts": {
        "typo3/cms": "<7.6.55 || >=8.0.0"
    }
}
